<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ifc.oob" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Oob!</name>
    <description>
        Beta Test
    </description>
    <author email="joaocampo2102@gmail.com" href="http://cordova.io">
        Jo√£o Vitor Maia
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon/android/icon.png" />
        <preference name="Fullscreen" value="false" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.1" />
    <plugin name="cordova-plugin-vibration" spec="^3.1.0" />
    <engine name="android" spec="^7.0.0" />
</widget>
